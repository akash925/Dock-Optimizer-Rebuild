  "scripts": {
    "dev": "concurrently -k -n server,client -c green,cyan \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx -r dotenv/config server/index.ts",
    "dev:client": "vite",

    "dev:doppler": "doppler run --config dev -- tsx server/index.ts",
    "dev:prod": "doppler --no-check-version run --config prd -- tsx server/index.ts",

    "build": "vite build && tsc -p tsconfig.build.json",
    "start": "NODE_ENV=production node dist/index.js",
    "start:doppler": "doppler --no-check-version run --config prd -- sh -c 'npm run build && node dist/index.js'",

    "lint": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --fix",

    "typecheck": "tsc -p tsconfig.soft.json",
    "test": "vitest",
    "test:watch": "vitest watch",

    "db:push": "drizzle-kit push",
    "db:migrate": "node scripts/run-migrations.cjs",
    "db:seed": "tsx server/seeds/20250623_us_holidays.ts"
  },
