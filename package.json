    "ts:autofix": "ts-autofix --apply --project tsconfig.server.json",
    "ts:lintfix":   "eslint \"src/**/*.{ts,tsx}\" --fix --quiet || true",
    "ts:codemods": "ts-node scripts/*.ts || true",
    "ts:cleanup": "pnpm run ts:migrate && pnpm run ts:autofix && pnpm run ts:lintfix && pnpm run ts:codemods"
