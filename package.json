  "scripts": {
    "test:ci": "vitest run --exclude '**/*.int.test.ts' --exclude '**/*.jest.test.ts'",
    "test:ci:watch": "vitest watch --exclude '**/*.int.test.ts' --exclude '**/*.jest.test.ts'",
    "dev:server": "tsx -r dotenv/config server/index.ts",
    "dev:client": "vite",
    "dev": "concurrently -k -n server,client -c green,cyan \"npm:dev:server\" \"npm:dev:client\"",
    "dev:prod": "NODE_ENV=production tsx -r dotenv/config server/index.ts",
    "production": "bash scripts/production-replit.sh",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc -p tsconfig.soft.json",
    "typecheck": "tsc -p tsconfig.soft.json",
    "lint": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.mjs,.cjs,.ts,.tsx --fix",
    "db:push": "drizzle-kit push",
    "db:migrate": "node scripts/run-migrations.cjs",
    "db:seed": "tsx server/seeds/20250623_us_holidays.ts",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:availability": "vitest run server/src/services/availability.test.ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:debug": "cypress open --env DEBUG_MODE=true,ENABLE_BREAKPOINTS=true",
    "e2e": "cypress run --spec cypress/e2e/**/*.spec.ts",
    "e2e:booking": "cypress run --spec cypress/e2e/booking-flow.spec.ts",
    "e2e:debug": "cypress open --spec cypress/e2e/debugging-breakpoints.spec.ts --env DEBUG_MODE=true",
    "e2e:modules": "cypress run --spec cypress/e2e/module_toggles.spec.ts"
  },
